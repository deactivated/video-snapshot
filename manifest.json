{
  "name": "YouTube Screenshot",
  "version": "0.9",

  "manifest_version": 2, 

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
    
  ],

  "browser_action": {
    "name": "Take video screenshot.",
    "default_icon": "icon.png"
  }
}
